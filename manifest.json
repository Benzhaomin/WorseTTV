{
  "manifest_version": 2,
  "name": "Worse TwitchTV",
  "version": "0.0.2",
  "browser_action": {
    "default_icon": "./data/img/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.twitch.tv/*"
      ],
      "css": [
        "./data/content-style.css"
      ],
      "js": [
        "./data/chat.js",
        "./data/chrome-chat.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "./data/chrome-background.js",
      "./data/chrome-ui.js",
      "./lib/diagnosis.js",
      "./lib/symptoms.js",
      "./lib/emotes.js"
    ]
  },
  "permissions": ["http://www.twitch.tv/*"]
}
